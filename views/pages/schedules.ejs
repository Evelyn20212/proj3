<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Schedule website</h1>
    <table>
        <thead>
            <tr>
                <th>user_id</th>
                <th>day</th>
                <th>start_at</th>
                <th>end_at</th>
            </tr>
        </thead>
        <tbody>
            <% for( let index = 0; index < schedules.length; index++ ) { %>
            <tr>
                <td><%= schedules[index].user_id %></td>
                <td><%= schedules[index].day %></td>
                <td><%= schedules[index].start_at %></td>
                <td><%= schedules[index].end_at %></td>
                
            </tr> 

            <% } %>
        </tbody>
    </table>
</body>
</html>